{
	"name": "ryanbakes-monorepo",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.27.0",
	"engines": {
		"node": "v22.x"
	},
	"scripts": {
		"build": "pnpm -r --filter=!ryanbakes-monorepo build",
		"dev": "pnpm --filter website dev",
		"format": "pnpm -r --filter=!ryanbakes-monorepo format",
		"lint": "pnpm -r --filter=!ryanbakes-monorepo lint",
		"preinstall": "corepack enable",
		"prepare": "husky",
		"typecheck": "pnpm -r --filter=!ryanbakes-monorepo typecheck",
		"validate:env": "pnpm --filter website validate:env"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7"
	}
}
